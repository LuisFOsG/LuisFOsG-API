{
  "name": "api-luisfosg",
  "version": "1.0.0",
  "description": "Data API from LuisFOsG",
  "main": "index.js",
  "scripts": {
    "copy": "copyfiles --up 1 src/public/* src/public/images/* dist",
    "dev": "cross-env NODE_ENV=development nodemon src/index.js --exec babel-node",
    "build": "cross-env NODE_ENV=production babel src --out-dir dist && npm run copy",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "lint": "eslint . --fix",
    "test": "cross-env NODE_ENV=test jest --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LuisFOsG/LuisFOsG-API.git"
  },
  "keywords": [],
  "author": "LuisFOsG",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/LuisFOsG/LuisFOsG-API/issues"
  },
  "homepage": "https://github.com/LuisFOsG/LuisFOsG-API#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "core-js": "^3.9.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "cryptr": "^6.0.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "fs-extra": "^9.1.0",
    "helmet": "^4.4.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.11.19",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "nodemailer": "^6.5.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.0",
    "@babel/core": "^7.13.8",
    "@babel/node": "^7.13.0",
    "@babel/preset-env": "^7.13.9",
    "copyfiles": "^2.4.1",
    "eslint": "^7.22.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7"
  },
  "jest": {
    "testEnvironment": "node"
  }
}
