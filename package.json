{
  "name": "api-luisfosg",
  "version": "1.0.0",
  "description": "Data API from LuisFOsG",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "build": "cross-env NODE_ENV=production babel src --out-dir dist && npm run copy",
    "dev": "cross-env NODE_ENV=development nodemon src/index.js --exec babel-node",
    "test": "cross-env NODE_ENV=test jest --verbose",
    "lint": "eslint . --fix",
    "copy": "copyfiles --up 1 src/public/* src/public/images/* dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LuisFOsG/LuisFOsG-API.git"
  },
  "keywords": [],
  "author": "LuisFOsG",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/LuisFOsG/LuisFOsG-API/issues"
  },
  "homepage": "https://github.com/LuisFOsG/LuisFOsG-API#readme",
  "dependencies": {
    "bcryptjs": "2.4.3",
    "core-js": "3.15.2",
    "cors": "2.8.5",
    "cross-env": "7.0.3",
    "cryptr": "6.0.2",
    "dotenv": "10.0.0",
    "express": "4.17.1",
    "fs-extra": "10.0.0",
    "helmet": "4.6.0",
    "jsonwebtoken": "8.5.1",
    "mongoose": "5.13.2",
    "morgan": "1.10.0",
    "multer": "1.4.2",
    "nodemailer": "6.6.2"
  },
  "devDependencies": {
    "@babel/cli": "7.14.5",
    "@babel/core": "7.14.6",
    "@babel/node": "7.14.7",
    "@babel/preset-env": "7.14.7",
    "copyfiles": "2.4.1",
    "eslint": "7.30.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-plugin-import": "2.23.4",
    "jest": "27.0.6",
    "nodemon": "2.0.12",
    "supertest": "6.1.3"
  },
  "jest": {
    "testEnvironment": "node"
  }
}
